{
	"name": "instagram-ts",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"format": "prettier --write .",
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/cli.js",
		"test": "prettier --check . && xo && ava",
		"prepare": "cd .. && husky instagram-ts/.husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,md}": [
			"prettier --write"
		]
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"@types/js-yaml": "^4.0.9",
		"ascii-art": "^2.8.5",
		"ink": "^5.2.1",
		"ink-testing-library": "^4.0.0",
		"ink-text-input": "^6.0.0",
		"instagram-private-api": "^1.46.1",
		"js-yaml": "^4.1.0",
		"meow": "^11.0.0",
		"pastel": "^3.0.0",
		"react": "^18.2.0",
		"readline": "^1.3.0",
		"replied": "^1.0.1",
		"sharp": "^0.34.3",
		"sixel": "^0.16.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node-fetch": "^2.6.12",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
